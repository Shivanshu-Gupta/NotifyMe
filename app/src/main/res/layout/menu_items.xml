<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:circle="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.szugyi.circlemenu.view.CircleImageView
        android:id="@+id/main_food"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/circle"
        android:elevation="2dp"
        android:src="@drawable/ic_food"
        circle:name="food" />

    <com.szugyi.circlemenu.view.CircleImageView
        android:id="@+id/main_travel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/circle"
        android:elevation="2dp"
        android:src="@drawable/ic_cab"
        circle:name="travel" />

    <com.szugyi.circlemenu.view.CircleImageView
        android:id="@+id/main_clothing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/circle"
        android:elevation="2dp"
        android:src="@drawable/ic_clothing"
        circle:name="clothing" />

    <com.szugyi.circlemenu.view.CircleImageView
        android:id="@+id/main_accessories"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/circle"
        android:src="@drawable/ic_accessories"
        circle:name="accessories" />

    <com.szugyi.circlemenu.view.CircleImageView
        android:id="@+id/main_movies"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/circle"
        android:elevation="2dp"
        android:src="@drawable/ic_movies"
        circle:name="movies" />

    <com.szugyi.circlemenu.view.CircleImageView
        android:id="@+id/main_misc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/circle"
        android:elevation="2dp"
        android:src="@drawable/ic_star"
        circle:name="misc" />

</merge>